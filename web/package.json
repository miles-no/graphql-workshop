{
  "name": "web",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "prestart": "yarn codegen",
    "start": "concurrently npm:start:run npm:start:codegen",
    "start:run": "vite",
    "start:codegen": "yarn codegen --watch",
    "build": "tsc && vite build",
    "lint": "eslint --ext .ts,.tsx src",
    "serve": "vite preview",
    "format": "prettier --write src",
    "codegen": "graphql-codegen --config codegen.yml",
    "download-schema": "npx apollo client:download-schema --endpoint=http://localhost:5000/graphql ../schema.graphql"
  },
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.1",
